    "autoprefixer": "^10.4.21",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
