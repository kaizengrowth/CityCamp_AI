    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.48.0",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^9.0.0",
    "react-pdf": "^10.0.1",
