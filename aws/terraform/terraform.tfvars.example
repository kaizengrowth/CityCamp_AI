# Copy this file to terraform.tfvars and fill in your values

aws_region = "us-east-2"
project_name = "citycamp-ai"

# EC2 configuration
ec2_instance_type = "t3.medium"
key_pair_name = "" # Optional: specify if you want SSH access

# SSL Certificate ARN (replace with your certificate ARN)
ssl_certificate_arn = "arn:aws:acm:us-east-2:YOUR_ACCOUNT_ID:certificate/YOUR_CERTIFICATE_ID"

# Optional: Override defaults
# ec2_instance_type = "t3.medium"
