# Copy this file to terraform.tfvars and fill in your values

aws_region = "us-east-2"
project_name = "citycamp-ai"

# SENSITIVE: Your domain name (replace with your actual domain)
domain_name = "yourdomain.com"

# SENSITIVE: Your AWS account ID (replace with your actual account ID)
aws_account_id = "123456789012"

# SENSITIVE: Git repository URL for application deployment
repository_url = "https://github.com/yourusername/yourrepo.git"

# EC2 configuration
ec2_instance_type = "t3.medium"
instance_type = "t3.medium"
key_pair_name = "" # Optional: specify if you want SSH access
ec2_key_name = null # Optional: specify if you want SSH access

# Optional: Override defaults
# ec2_instance_type = "t3.large"
