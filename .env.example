# Environment Variables for CityCamp AI Deployment
# Copy this file to .env and fill in your secure values

# Database Configuration
TF_VAR_db_password=your-secure-database-password-here

# AWS Configuration
AWS_REGION=us-east-2
AWS_ACCOUNT_ID=538569249671

# Deployment Secrets (store in AWS Systems Manager Parameter Store)
# These will be automatically fetched during deployment
OPENAI_API_KEY=your-openai-api-key-here
TWILIO_ACCOUNT_SID=your-twilio-account-sid-here
TWILIO_AUTH_TOKEN=your-twilio-auth-token-here
TWILIO_PHONE_NUMBER=your-twilio-phone-number-here
SMTP_USERNAME=your-smtp-username-here
SMTP_PASSWORD=your-smtp-password-here
GEOCODIO_API_KEY=your-geocodio-api-key-here

# Application Security
SECRET_KEY=your-secure-secret-key-here
